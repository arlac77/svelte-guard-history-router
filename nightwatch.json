{
  "src_folders": ["tests"],
  "output_folder": "result",

  "test_runner": {
    "type": "mocha",
    "options": {
      "ui": "bdd",
      "reporter": "list"
    }
  },

  "webdriver": {
    "start_process": true,
    "server_path": "/usr/bin/safaridriver",
    "port": 9515
  },

  "test_settings": {
    "default": {
      "desiredCapabilities": {
        "browserName": "safari"
      }
    }
  }
}
