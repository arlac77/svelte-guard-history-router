{
  "name": "svelte-guard-history-router",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "svelte": "./src/index.svelte",
  "description": "svelte router for SPA (history mode only)",
  "keywords": [
    "component",
    "router",
    "spa",
    "svelte",
    "web"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c example/rollup.config.mjs -w",
    "test": "ava --timeout 2m && npm run testcafe && testcafe $BROWSER:headless tests/*-test.js -s test-results --app-init-delay 1000 --app \"rollup -c example/rollup.config.mjs -w\"",
    "docs": "documentation readme ./src/index.mjs --section=API && documentation readme {{main}} --section=API",
    "lint": "documentation lint ./src/index.mjs && documentation lint {{main}}",
    "testcafe": "testcafe $BROWSER:headless testcafe/*.js -s test-results --app-init-delay 5000 --app \"rollup -c example/rollup.config.mjs -w\""
  },
  "dependencies": {
    "multi-path-matcher": "^2.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "ava": "^3.11.0",
    "documentation": "^13.0.2",
    "rollup": "^2.25.0",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-svelte": "git+https://github.com/ctavan/rollup-plugin-svelte.git#handle-missing-exports-gracefully",
    "semantic-release": "^17.1.1",
    "svelte": "^3.24.0",
    "testcafe": "^1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-guard-history-router.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-guard-history-router/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-guard-history-router#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-component"
    ]
  }
}
